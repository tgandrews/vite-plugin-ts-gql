{
  "name": "vite-plugin-ts-gql",
  "version": "0.1.0",
  "contributors": [
    "Tom Andrews <git@tomandrews.co.uk>"
  ],
  "description": "Vite plugin for starting ts-gql in dev",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "format": "prettier **/*.ts",
    "release": "np --no-publish --branch=main"
  },
  "repository": "git://github.com/tgandrews/vite-plugin-ts-gql.git",
  "keywords": [
    "vite",
    "ts-gql"
  ],
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "husky": "^8.0.1",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.1",
    "np": "^7.6.3",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "@ts-gql/compiler": "^0.16.3",
    "vite": "^4.1.2"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "prettier --write"
    ]
  }
}
